function Profile(){var e=require("ui/customUI/CustomButton"),t=new e;t.initialize("Classifiche",25);var i=Titanium.UI.createWindow({navTintColor:"white",tintColor:"#E2BB5A",barColor:"#E2BB5A",extendEdges:[Ti.UI.EXTEND_EDGE_TOP],backgroundColor:"#F5F4F2",translucent:!0,statusBarStyle:Titanium.UI.iPhone.StatusBar.LIGHT_CONTENT,layout:"composite",backButtonTitle:String.format(L("main:back"),"")}),o=Titanium.UI.createScrollView({backgroundColor:"#F5F4F2",width:Ti.UI.SIZE,height:Ti.UI.SIZE,layout:"vertical",top:65}),n=Titanium.Filesystem.getFile(Titanium.Filesystem.applicationDataDirectory,"profile_name.png"),a=Titanium.UI.createView({backgroundColor:"transparent",backgroundImage:n.getNativePath(),width:200,height:200,top:20}),r=Titanium.UI.createView({width:200,height:200,backgroundImage:"images/frame.png",backgroundColor:"transparent"}),l=Titanium.UI.createLabel({top:20,font:{fontSize:25,fontFamily:"Roboto-Regular"},text:Ti.App.Properties.getString("fb_first_name")+" "+Ti.App.Properties.getString("fb_last_name"),textAlign:"center",color:"#856C64",width:"95%"}),s=Titanium.UI.createLabel({text:"x",font:{fontSize:110,fontFamily:"Sughayer Separates 4"},color:"#856C64",top:-80}),d=Titanium.UI.createLabel({top:10,font:{fontSize:20,fontFamily:"Roboto-Light"},text:"Vittorie: ",textAlign:"center",color:"#856C64",width:"95%"}),u=Titanium.UI.createLabel({top:10,font:{fontSize:20,fontFamily:"Roboto-Light"},text:"Sconfitte: ",textAlign:"center",color:"#856C64",width:"95%"}),c=Titanium.UI.createLabel({top:10,font:{fontSize:20,fontFamily:"Roboto-Light"},text:"Mancati: ",textAlign:"center",color:"#856C64",width:"95%"}),h=Titanium.UI.createButton({title:"Dettagli",font:{fontSize:21},color:"#856C64",top:10}),T=Titanium.UI.createButton({title:"Logout",font:{fontSize:21},color:"#856C64",top:10});T.addEventListener("click",function(){}),h.addEventListener("click",function(){var e=require("ui/profile/Details"),t=new e;global.mainTab.open(t,{transition:Ti.UI.iPhone.AnimationStyle.FLIP_FROM_LEFT})}),t.addEventListener("click",function(){var e=require("ui/profile/Charts");global.mainTab.open(new e,{transition:Ti.UI.iPhone.AnimationStyle.FLIP_FROM_LEFT})});var m=Titanium.UI.createLabel({top:10,font:{fontSize:20,fontFamily:"Roboto-Light"},text:"Velocit√† media: ",textAlign:"center",color:"#856C64",width:"95%"});return a.add(r),o.add(a),o.add(l),o.add(s),i.add(o),loader.showLoader(i),i.addEventListener("open",function(){var e=Ti.Network.createHTTPClient({onload:function(){Ti.API.info("Received text: "+this.responseText);var e=JSON.parse(this.responseText);d.text+=e[0].num_winner_matches,o.add(d),u.text+=e[0].num_loose_matches,o.add(u),c.text+=e[0].num_faults,o.add(c),m.text+=e[0].acceleration,o.add(m),o.add(h),o.add(t),loader.hideLoader(i)},onerror:function(e){Ti.API.debug(e.error),loader.hideLoader(i)},timeout:5e3}),n={id_player:Ti.App.Properties.getString("fb_id")};Ti.API.info(JSON.stringify(n)),e.open("GET",global.bangServerUrl+"/stats-getStats.php?stats="+JSON.stringify(n)),e.send()}),i}module.exports=Profile;