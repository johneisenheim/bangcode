function Details(){var e=require("ui/customUI/PageTitle"),t=new e;t.initialize("Duelli",10);var i=null,o=null,n=Titanium.UI.createWindow({navTintColor:"white",tintColor:"#E2BB5A",barColor:"#E2BB5A",extendEdges:[Ti.UI.EXTEND_EDGE_TOP],backgroundColor:"#F5F4F2",translucent:!0,statusBarStyle:Titanium.UI.iPhone.StatusBar.LIGHT_CONTENT,layout:"composite",backButtonTitle:String.format(L("main:back"),"")}),a=Titanium.UI.createScrollView({backgroundColor:"#F5F4F2",width:Ti.UI.SIZE,height:Ti.UI.SIZE,layout:"vertical",top:65}),r=Titanium.UI.createView({backgroundColor:"#E2BB5A",width:"100%",height:30,layout:"horizontal"}),l=Titanium.UI.createLabel({left:10,top:3,font:{fontSize:19,fontFamily:"Roboto-Light"},text:"Sfide vinte",textAlign:"center",color:"white"});r.add(l);var d=Titanium.UI.createView({backgroundColor:"#E2BB5A",width:"100%",height:30,layout:"horizontal"}),s=Titanium.UI.createLabel({left:10,top:3,font:{fontSize:19,fontFamily:"Roboto-Light"},text:"Sfide perse",textAlign:"center",color:"white"});return d.add(s),loader.showLoader(n),n.addEventListener("open",function(){var e=Ti.Network.createHTTPClient({onload:function(){Ti.API.info("Received text: "+this.responseText);for(var e=JSON.parse(this.responseText),t=[],l=[],s=Ti.App.Properties.getString("fb_id"),c=0;c<e.length;c++)e[c].idMaster==s?0==e[c].winner?t.push({title:e[c].playerTwo+" il "+e[c].challenge_date,height:45,color:"#856C64",font:{fontSize:18,fontFamily:"Roboto-Light"}}):l.push({title:e[c].playerTwo+" il "+e[c].challenge_date,height:45,color:"#856C64",font:{fontSize:18,fontFamily:"Roboto-Light"}}):0==e[c].winner?t.push({title:e[c].playerOne+" il "+e[c].challenge_date,height:45,color:"#856C64",font:{fontSize:18,fontFamily:"Roboto-Light"}}):l.push({title:e[c].playerOne+" il "+e[c].challenge_date,height:45,color:"#856C64",font:{fontSize:18,fontFamily:"Roboto-Light"}});if(i=Titanium.UI.createTableView({top:25,width:"100%",height:45*t.length+30,showVerticalScrollIndicator:!0,separatorColor:"#B8B1AF",backgroundColor:"#FDFDFD",headerView:r,data:t,scrollable:!1,selectionStyle:Titanium.UI.iPhone.TableViewCellSelectionStyle.GRAY}),o=Titanium.UI.createTableView({top:25,width:"100%",height:45*l.length+30,showVerticalScrollIndicator:!0,separatorColor:"#B8B1AF",backgroundColor:"#FDFDFD",headerView:d,data:l,scrollable:!1,selectionStyle:Titanium.UI.iPhone.TableViewCellSelectionStyle.GRAY}),0==t.length)if(0==l.length){var u=Titanium.UI.createLabel({top:"30%",font:{fontSize:19,fontFamily:"Roboto-Light"},text:"Non ci sono sfide effettuate.",textAlign:"center",color:"#aa938b",width:"95%"});a.add(u)}else a.add(o);else 0==l.length?a.add(i):(a.add(i),a.add(o));loader.hideLoader(n)},onerror:function(e){Ti.API.debug(e.error),loader.hideLoader(n)},timeout:5e3}),t={id_player:Ti.App.Properties.getString("fb_id")};Ti.API.info(JSON.stringify(t)),e.open("GET",global.bangServerUrl+"/churchbell-getChurchbell.php?churchbell="+JSON.stringify(t)),e.send()}),a.add(t),n.add(a),n}module.exports=Details;