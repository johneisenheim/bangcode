function ChallengeWindow(){{var e=require("ui/challenges/ChallengeView"),t=new e,i=Titanium.UI.createWindow({navTintColor:"white",tintColor:"#E2BB5A",barColor:"#E2BB5A",extendEdges:[Ti.UI.EXTEND_EDGE_TOP],backgroundColor:"#F5F4F2",translucent:!0,statusBarStyle:Titanium.UI.iPhone.StatusBar.LIGHT_CONTENT,layout:"composite",backButtonTitle:String.format(L("main:back"),""),tabBarHidden:!0});Titanium.UI.createActivityIndicator({style:Titanium.UI.iPhone.ActivityIndicatorStyle.DARK})}return i.add(t),i}module.exports=ChallengeWindow;