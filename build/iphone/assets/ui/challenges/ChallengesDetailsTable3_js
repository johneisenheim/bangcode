function ChallengesDetailsTable3(){var e=require("ui/customUI/BigTile"),i=new e;i.initialize("tileTitle",15,19);var t=require("ui/customUI/PageTitle"),n=new t;n.initialize("Duello",75);var a=require("ui/customUI/MinusNormalTile"),o=new a,r=Titanium.UI.createWindow({navTintColor:"white",tintColor:"#E2BB5A",barColor:"#E2BB5A",extendEdges:[Ti.UI.EXTEND_EDGE_TOP],backgroundColor:"#F5F4F2",translucent:!0,statusBarStyle:Titanium.UI.iPhone.StatusBar.LIGHT_CONTENT,layout:"composite",backButtonTitle:String.format(L("main:back"),""),tabBarHidden:!0}),l=Titanium.UI.createScrollView({width:"100%",height:"100%",backgroundColor:"#F5F4F2",layout:"vertical"}),s=(Titanium.UI.createLabel({text:"",top:15,font:{fontSize:24,fontFamily:"Roboto-Light"},textAlign:"center",color:"#856C64",width:"95%"}),Titanium.UI.createButton({title:"Annulla richiesta",font:{fontSize:21},color:"#856C64",top:30}));return s.addEventListener("click",function(){var e=Ti.UI.createAlertDialog({cancel:1,buttonNames:["Si, annulla","Ci ho ripensato"],message:"Sei sicuro di voler annullare la richiesta?",title:"Annulla richiesta"});e.show(),e.addEventListener("click",function(e){if(e.index!==e.source.cancel){loader.showLoader(r);var i=Ti.Network.createHTTPClient({onload:function(){Ti.API.info("Received text: "+this.responseText);var e=JSON.parse(this.responseText);Ti.API.info(e.return),"0"===e.return?(loader.hideLoader(r),Ti.API.info("ok"),Ti.App.fireEvent("reload",{}),setTimeout(function(){r.close()},1e3)):(loader.hideLoader(r),alert("Error"))},onerror:function(e){loader.hideLoader(r),Ti.API.debug(e.error)},timeout:5e3}),t={id:matchID};Ti.API.info(JSON.stringify(t)),i.open("GET",global.bangServerUrl+"/challenge-rejectChallenge.php?id_challenge="+JSON.stringify(t)),i.send()}})}),r.initialize=function(e){i.setTitle(e.title+" nei paraggi di "+e.address);var t=e.challenge_date;Ti.API.info(t);var n=t.split(" "),a=n[0].split("-"),r=n[1].split(":");year=a[0],month=a[1],day=a[2],hours=r[0],minutes=r[1],seconds=r[2],latitude=e.latitude,longitude=e.longitude,matchID=e.id,o.initialize("il giorno "+day+"-"+month+"-"+year+" alle ore "+hours+":"+minutes,10,18)},l.add(n),l.add(i),l.add(o),l.add(s),r.add(l),r}module.exports=ChallengesDetailsTable3;