function Settings(){var e=10*Ti.App.Properties.getDouble("volume",.4),t=null,i=Titanium.UI.createWindow({navTintColor:"white",tintColor:"#E2BB5A",barColor:"#E2BB5A",extendEdges:[Ti.UI.EXTEND_EDGE_TOP],backgroundColor:"#F5F4F2",translucent:!0,statusBarStyle:Titanium.UI.iPhone.StatusBar.LIGHT_CONTENT,layout:"composite",backButtonTitle:String.format(L("main:back"),"")}),o=Titanium.UI.createScrollView({backgroundColor:"#F5F4F2",width:"100%",height:"100%",layout:"vertical"}),a=Titanium.UI.createLabel({top:80,font:{fontSize:20,fontFamily:"Roboto-Light"},text:"Volume Musica",textAlign:"center",color:"#856C64",width:"95%"}),n=Titanium.UI.createSlider({top:20,min:0,max:10,width:"70%",value:e}),r=Titanium.UI.createLabel({top:5,font:{fontSize:17,fontFamily:"Roboto-LightItalic"},text:"Il volume della musica si riferisce alla musica di sottofondo dell'applicazione.",textAlign:"center",color:"#aa938b",width:"95%"}),l=Titanium.UI.createLabel({top:10,font:{fontSize:20,fontFamily:"Roboto-Light"},text:"Volume Effetti",textAlign:"center",color:"#856C64",width:"93%"}),s=Titanium.UI.createSlider({top:20,min:0,max:10,width:"70%",value:5}),d=Titanium.UI.createLabel({top:5,font:{fontSize:17,fontFamily:"Roboto-LightItalic"},text:"Il volume degli effetti si riferisce agli effetti sonori che vengono riprodotti durante l'utilizzo dell'applicazione.",textAlign:"center",color:"#aa938b",width:"93%"}),c=Titanium.UI.createLabel({text:"x",font:{fontSize:100,fontFamily:"Sughayer Separates 4"},color:"#856C64",top:-55});n.addEventListener("change",function(e){t=(e.value/10).toFixed(1),player.volume=t,Ti.API.info(t)}),n.addEventListener("touchend",function(){Ti.App.Properties.setDouble("volume",t),Ti.API.info(player.volume)});var u=Titanium.UI.createView({backgroundColor:"#E2BB5A",width:"100%",height:30,layout:"horizontal"}),m=Titanium.UI.createLabel({left:10,top:3,font:{fontSize:19,fontFamily:"Roboto-Light"},text:"Musica",textAlign:"center",color:"white"});u.add(m);var T=Ti.UI.createTableViewSection({headerView:u}),h=Ti.UI.createTableViewRow({title:"Il buono, il brutto e il cattivo",height:45,color:"#856C64",font:{fontSize:18,fontFamily:"Roboto-Light"},index:1});T.add(h);var p=Ti.UI.createTableViewRow({title:"Per un pugno di dollari",height:45,color:"#856C64",font:{fontSize:18,fontFamily:"Roboto-Light"},index:2});T.add(p);var f=Ti.UI.createTableViewRow({title:"Lo chiamavano Trinit√†",height:45,color:"#856C64",font:{fontSize:18,fontFamily:"Roboto-Light"},index:3});T.add(f);var g=Ti.UI.createTableViewRow({title:"Bang Bang(My baby shot me down)",height:45,color:"#856C64",font:{fontSize:18,fontFamily:"Roboto-Light"},index:4});T.add(g);var v=Ti.UI.createTableViewRow({title:"Mashup",height:45,color:"#856C64",font:{fontSize:18,fontFamily:"Roboto-Light"},index:5});switch(T.add(v),track){case 1:h.hasCheck=!0,p.hasCheck=!1,f.hasCheck=!1,g.hasCheck=!1,v.hasCheck=!1;break;case 2:h.hasCheck=!1,p.hasCheck=!0,f.hasCheck=!1,g.hasCheck=!1,v.hasCheck=!1;break;case 3:h.hasCheck=!1,p.hasCheck=!1,f.hasCheck=!0,g.hasCheck=!1,v.hasCheck=!1;break;case 4:h.hasCheck=!1,p.hasCheck=!1,f.hasCheck=!1,g.hasCheck=!0,v.hasCheck=!1;break;case 5:h.hasCheck=!1,p.hasCheck=!1,f.hasCheck=!1,g.hasCheck=!1,v.hasCheck=!0}var I=Ti.UI.createTableView({data:[T],top:40,width:"100%",height:255,showVerticalScrollIndicator:!0,separatorColor:"#B8B1AF",backgroundColor:"#FDFDFD",scrollable:!1,selectionStyle:Titanium.UI.iPhone.TableViewCellSelectionStyle.GRAY});return I.addEventListener("click",function(e){switch(e.rowData.index){case 1:h.hasCheck=!0,p.hasCheck=!1,f.hasCheck=!1,g.hasCheck=!1,v.hasCheck=!1,player.stop(),Ti.App.Properties.setInt("track",1),player.media="music/track1.mp3",player.play();break;case 2:h.hasCheck=!1,p.hasCheck=!0,f.hasCheck=!1,g.hasCheck=!1,v.hasCheck=!1,player.stop(),Ti.App.Properties.setInt("track",2),player.media="music/track2.mp3",player.play();break;case 3:h.hasCheck=!1,p.hasCheck=!1,f.hasCheck=!0,g.hasCheck=!1,v.hasCheck=!1,player.stop(),Ti.App.Properties.setInt("track",3),player.me