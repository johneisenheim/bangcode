function MainWindow(){var e=require("ui/MainView"),i=new e,t=(require("bencoding.localnotify"),Titanium.UI.createButton({title:"Tutorial"}));t.addEventListener("click",function(){var e=require("ui/tutorial/Tutorial"),i=new e;i.open()});var n=Titanium.UI.createButton({backgroundColor:"transparent",backgroundImage:"images/settings.png",width:25,height:20});n.addEventListener("click",function(){var e=require("ui/settings/Settings"),i=new e;global.mainTab.open(i,{transition:Ti.UI.iPhone.AnimationStyle.FLIP_FROM_LEFT})});var o=Titanium.UI.createWindow({navTintColor:"white",extendEdges:[Ti.UI.EXTEND_EDGE_TOP],backgroundColor:"#F5F4F2",statusBarStyle:Titanium.UI.iPhone.StatusBar.LIGHT_CONTENT,layout:"vertical",tabBarHidden:!0,rightNavButton:n,leftNavButton:t});return o.add(i),o}module.exports=MainWindow;