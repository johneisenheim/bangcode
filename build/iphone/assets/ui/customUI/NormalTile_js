function NormalTile(){var t=Titanium.UI.createView({backgroundColor:"transparent",width:"95%",height:50}),e=Titanium.UI.createView({backgroundColor:"#FDFDFD",width:"100%",height:47,top:0}),i=Titanium.UI.createView({backgroundColor:"#E2E2E2",width:"100%",height:3,top:47}),o=Titanium.UI.createLabel({text:"",font:{fontSize:21,fontFamily:"Roboto-Light"},color:"#856C64",textAlign:"center"});return t.initialize=function(i,n,a){t.top=n,o.text=i,o.setFont({fontSize:a,fontFamily:"Roboto-Light"}),e.add(o)},t.setTitle=function(t){o.text=t},t.add(e),t.add(i),t}module.exports=NormalTile;