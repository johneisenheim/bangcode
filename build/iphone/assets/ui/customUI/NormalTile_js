function NormalTile(){var i=Titanium.UI.createView({backgroundColor:"transparent",width:"97%",height:50}),e=Titanium.UI.createView({backgroundColor:"#FDFDFD",width:"100%",height:47,top:0}),t=Titanium.UI.createView({backgroundColor:"#E2E2E2",width:"100%",height:3,top:47}),o=Titanium.UI.createLabel({text:"",font:{fontSize:21,fontFamily:"Roboto-Light"},color:"#856C64",textAlign:"center"});return i.initialize=function(t,n,r){i.top=n,o.text=t,o.setFont({fontSize:r,fontFamily:"Roboto-Light"}),e.add(o)},i.setTitle=function(i){o.text=i},i.add(e),i.add(t),i}module.exports=NormalTile;