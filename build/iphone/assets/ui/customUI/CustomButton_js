function CustomButton(){var e=Titanium.UI.createView({backgroundColor:"transparent",width:200,height:45,canFire:!0}),t=Titanium.UI.createView({backgroundColor:"#F7CD63",width:"100%",height:42,top:0}),i=Titanium.UI.createView({backgroundColor:"#B49138",width:"100%",height:3,top:42}),n=Titanium.UI.createLabel({text:"",font:{fontSize:21,fontFamily:"Roboto-Regular"},color:"white",textAlign:"center"});return e.initialize=function(i,a){e.top=a,n.text=i,t.add(n)},e.disable=function(){t.backgroundColor="#D1CBBD",e.canFire=!1},e.add(t),e.add(i),e}module.exports=CustomButton;