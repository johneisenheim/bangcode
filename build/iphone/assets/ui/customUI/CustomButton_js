function CustomButton(){var i=Titanium.UI.createView({backgroundColor:"transparent",width:200,height:45}),e=Titanium.UI.createView({backgroundColor:"#F7CD63",width:"100%",height:42,top:0}),t=Titanium.UI.createView({backgroundColor:"#B49138",width:"100%",height:3,top:42}),o=Titanium.UI.createLabel({text:"",font:{fontSize:21,fontFamily:"Roboto-Regular"},color:"white",textAlign:"center"});return i.initialize=function(t,n){i.top=n,o.text=t,e.add(o)},i.add(e),i.add(t),i}module.exports=CustomButton;