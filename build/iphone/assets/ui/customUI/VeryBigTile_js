function VeryBigTile(){var e=Titanium.UI.createView({backgroundColor:"transparent",width:"90%",height:120}),t=Titanium.UI.createView({backgroundColor:"#FDFDFD",width:"100%",height:117,top:0}),i=Titanium.UI.createView({backgroundColor:"#E2E2E2",width:"100%",height:3,top:117}),n=Titanium.UI.createLabel({text:"",font:{fontSize:18,fontFamily:"Roboto-LightItalic"},color:"#856C64",textAlign:"center"});return e.initialize=function(i,a){e.top=a,n.text=i,t.add(n)},e.setTitle=function(e){n.text=e},e.add(t),e.add(i),e}module.exports=VeryBigTile;