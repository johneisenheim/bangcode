function CustomDisabledButton(){var t=Titanium.UI.createView({backgroundColor:"transparent",width:200,height:45}),e=Titanium.UI.createView({backgroundColor:"#D1CBBD",width:"100%",height:42,top:0}),i=Titanium.UI.createView({backgroundColor:"#B49138",width:"100%",height:3,top:42}),o=Titanium.UI.createLabel({text:"",font:{fontSize:21,fontFamily:"Roboto-Regular"},color:"white",textAlign:"center"});return t.initialize=function(i,n){t.top=n,o.text=i,e.add(o)},t.enable=function(){e.backgroundColor="#F7CD63"},t.add(e),t.add(i),t}module.exports=CustomDisabledButton;