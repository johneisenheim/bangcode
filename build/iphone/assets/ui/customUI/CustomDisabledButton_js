function CustomDisabledButton(){var e=Titanium.UI.createView({backgroundColor:"transparent",width:200,height:45,isEnabled:!1}),t=Titanium.UI.createView({backgroundColor:"#D1CBBD",width:"100%",height:42,top:0}),i=Titanium.UI.createView({backgroundColor:"#B49138",width:"100%",height:3,top:42}),n=Titanium.UI.createLabel({text:"",font:{fontSize:21,fontFamily:"Roboto-Regular"},color:"white",textAlign:"center"});return e.initialize=function(i,a){e.top=a,n.text=i,t.add(n)},e.enable=function(){t.backgroundColor="#F7CD63",e.isEnabled=!0},e.add(t),e.add(i),e}module.exports=CustomDisabledButton;