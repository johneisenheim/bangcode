function MinusNormalTile(){var e=Titanium.UI.createView({backgroundColor:"transparent",width:"90%",height:50}),t=Titanium.UI.createView({backgroundColor:"#FDFDFD",width:"100%",height:47,top:0}),i=Titanium.UI.createView({backgroundColor:"#E2E2E2",width:"100%",height:3,top:47}),n=Titanium.UI.createLabel({text:"",font:{fontSize:21,fontFamily:"Roboto-Light"},color:"#856C64",textAlign:"center"});return e.initialize=function(i,o,a){e.top=o,n.text=i,n.setFont({fontSize:a,fontFamily:"Roboto-Light"}),t.add(n)},e.setTitle=function(e){n.text=e},e.getTitle=function(){return n.text},e.add(t),e.add(i),e}module.exports=MinusNormalTile;