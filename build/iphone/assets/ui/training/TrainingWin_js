function TrainingWin(e){var t=require("ui/customUI/CustomButton"),i=new t;i.initialize("Riprova",30);var o=ZLSound.createSample({media:"music/winSound.mp3",volume:1}),n=Titanium.UI.createScrollView({width:"100%",height:"100%",backgroundColor:"#F5F4F2",layout:"vertical"}),a=Titanium.UI.createImageView({top:50,image:"images/winIMG.png",backgroundColor:"transparent",width:johnsLib.calculateWidth(260),height:johnsLib.calculateHeight(357)}),r=Titanium.UI.createLabel({text:"Bang!",top:30,font:{fontSize:80,fontFamily:"Western"},textAlign:"center",color:"#856C64",width:"100%"}),l=Titanium.UI.createLabel({top:0,font:{fontSize:17,fontFamily:"Roboto-LightItalic"},text:"Hai impiegato "+myTime+" ms per sparare.",textAlign:"center",color:"#aa938b",width:"85%"}),s=Titanium.UI.createButton({title:"Chiudi",font:{fontSize:19},color:"#856C64",top:10}),d=Titanium.UI.createLabel({text:"x",font:{fontSize:130,fontFamily:"Sughayer Separates 4"},color:"#856C64",top:-95});return s.addEventListener("click",function(){e.close()}),n.playSound=function(){setTimeout(function(){o.play()},1500)},i.addEventListener("click",function(){e.close(),Ti.App.fireEvent("training_reload",{})}),n.add(a),n.add(r),n.add(d),n.add(l),n.add(i),n.add(s),n}module.exports=TrainingWin;