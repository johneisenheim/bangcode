function MainView(){var e=require("ui/customUI/NormalTile"),i=new e,t=new e,n=new e,a=null,o=null,r=Titanium.UI.createScrollView({backgroundColor:"#F5F4F2",width:"100%",height:"100%",layout:"vertical",top:10}),s=Titanium.UI.createLabel({text:"B!",font:{fontSize:310,fontFamily:"Outlaw"},color:"#856C64",top:15}),l=Titanium.UI.createLabel({text:"x",font:{fontSize:140,fontFamily:"Sughayer Separates 4"},color:"#856C64",top:-165}),c=Titanium.UI.createLabel({text:"western duels",font:{fontSize:65,fontFamily:"Western"},color:"#856C64",textAlign:"center",width:"70%",top:-30}),d=Titanium.UI.createButton({title:"Quick Game Finder",font:{fontSize:19},color:"#856C64",top:10}),u=Titanium.UI.createView({width:1,height:15,backgroundColor:"#F5F4F2"});return i.initialize(String.format(L("main:duelLabel"),""),35,21),t.initialize(String.format(L("main:challengesLabel"),""),10,21),n.initialize(String.format(L("main:profilestatsLabel"),""),10,21),r.add(s),r.add(l),r.add(c),r.add(i),r.add(t),r.add(n),r.add(d),r.add(u),d.addEventListener("click",function(){a=require("ui/quickGame/QuickGameFinder"),o=new a,o.open({modal:!0,modalTransitionStyle:Ti.UI.iPhone.MODAL_TRANSITION_STYLE_COVER_VERTICAL,modalStyle:Ti.UI.iPhone.MODAL_PRESENTATION_FORMSHEET})}),i.addEventListener("click",function(){var e=require("ui/newChallenge/StartChallengeWindow"),i=new e;global.mainTab.open(i,{transition:Ti.UI.iPhone.AnimationStyle.FLIP_FROM_LEFT})}),t.addEventListener("click",function(){var e=require("ui/challenges/ChallengeWindow"),i=new e;global.mainTab.open(i,{transition:Ti.UI.iPhone.AnimationStyle.FLIP_FROM_LEFT})}),n.addEventListener("click",function(){var e=require("ui/profile/Profile"),i=new e;global.mainTab.open(i,{transition:Ti.UI.iPhone.AnimationStyle.FLIP_FROM_LEFT})}),Ti.App.addEventListener("reloadFinderView",function(){o.close(),a=null,o=null,loader.reloadAnimation(),a=require("ui/quickGame/QuickGameFinder"),o=new a,o.open({modal:!0,modalTransitionStyle:Ti.UI.iPhone.MODAL_TRANSITION_STYLE_COVER_VERT