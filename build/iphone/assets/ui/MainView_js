function MainView(){var e=require("ui/customUI/NormalTile"),i=new e,n=new e,t=new e,o=require("login/facebook/FacebookHandler"),r=new o,a=Titanium.UI.createScrollView({backgroundColor:"#F5F4F2",width:Ti.UI.SIZE,height:Ti.UI.SIZE,layout:"vertical"}),l=Titanium.UI.createLabel({text:"B!",font:{fontSize:310,fontFamily:"Outlaw"},color:"#856C64",top:15}),s=Titanium.UI.createLabel({text:"x",font:{fontSize:140,fontFamily:"Sughayer Separates 4"},color:"#856C64",top:-165}),u=Titanium.UI.createLabel({text:"western duels",font:{fontSize:65,fontFamily:"Western"},color:"#856C64",textAlign:"center",width:"70%",top:-30});return i.initialize(String.format(L("main:duelLabel"),""),45,21),n.initialize(String.format(L("main:challengesLabel"),""),10,21),t.initialize(String.format(L("main:profilestatsLabel"),""),10,21),a.add(l),a.add(s),a.add(u),a.add(i),a.add(n),a.add(t),i.addEventListener("click",function(){var e=require("ui/newChallenge/StartChallengeWindow"),i=new e;global.mainTab.open(i,{transition:Ti.UI.iPhone.AnimationStyle.FLIP_FROM_LEFT})}),n.addEventListener("click",function(){var e=require("ui/challenges/ChallengeWindow"),i=new e;global.mainTab.open(i,{transition:Ti.UI.iPhone.AnimationStyle.FLIP_FROM_LEFT})}),t.addEventListener("click",function(){r.logMeOut()}),a}module.exports=MainView;