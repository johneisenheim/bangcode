function MainView(){var i=require("ui/customUI/NormalTile"),e=new i,t=new i,n=new i,o=Titanium.UI.createScrollView({backgroundColor:"#F5F4F2",width:Ti.UI.SIZE,height:Ti.UI.SIZE,layout:"vertical"}),a=Titanium.UI.createLabel({text:"B!",font:{fontSize:310,fontFamily:"Outlaw"},color:"#856C64",top:15}),r=Titanium.UI.createLabel({text:"x",font:{fontSize:140,fontFamily:"Sughayer Separates 4"},color:"#856C64",top:-165}),l=Titanium.UI.createLabel({text:"western duels",font:{fontSize:65,fontFamily:"Western"},color:"#856C64",textAlign:"center",width:"70%",top:-30});return e.initialize(String.format(L("main:duelLabel"),""),45,21),t.initialize(String.format(L("main:challengesLabel"),""),10,21),n.initialize(String.format(L("main:profilestatsLabel"),""),10,21),o.add(a),o.add(r),o.add(l),o.add(e),o.add(t),o.add(n),e.addEventListener("click",function(){Ti.App.fireEvent("goto_page",{what:"start_duel"})}),t.addEventListener("click",function(){Ti.App.fireEvent("goto_page",{what:"challenges"})}),o}module.exports=MainView;