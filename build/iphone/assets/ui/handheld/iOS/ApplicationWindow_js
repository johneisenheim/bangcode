function ApplicationWindow(){var i=require("ui/handheld/iOS/MainWindow"),e=new i,t=Titanium.UI.createTabGroup({navTintColor:"white",tintColor:"#E2BB5A",barColor:"#E2BB5A",translucent:!0}),n=Titanium.UI.createTab({window:e});return t.addTab(n),t.setActiveTab(n),Ti.App.addEventListener("goto_page",function(i){switch(Ti.API.info(i.what),i.what){case"challenges":var e=require("ui/handheld/iOS/challenges/ChallengeWindow"),t=new e;n.open(t,{transition:Ti.UI.iPhone.AnimationStyle.FLIP_FROM_LEFT});break;case"start_duel":var o=require("ui/handheld/iOS/newChallenge/StartChallengeWindow"),a=new o;n.open(a,{transition:Ti.UI.iPhone.AnimationStyle.FLIP_FROM_LEFT})}}),t}module.exports=ApplicationWindow;