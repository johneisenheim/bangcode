function Win(e){var t=require("ui/customUI/CustomButton"),i=new t;i.initialize("Rivincita",40);var o=ZLSound.createSample({media:"music/winSound.mp3",volume:1}),n=Titanium.UI.createScrollView({width:"100%",height:"100%",backgroundColor:"#F5F4F2",layout:"vertical"}),a=Titanium.UI.createImageView({top:50,image:"images/winIMG.png",backgroundColor:"transparent",width:johnsLib.calculateWidth(250),height:johnsLib.calculateHeight(300)}),r=Titanium.UI.createLabel({text:"Hai vinto!",top:30,font:{fontSize:80,fontFamily:"Western"},textAlign:"center",color:"#856C64",width:"100%"}),l=Titanium.UI.createLabel({top:5,font:{fontSize:17,fontFamily:"Roboto-LightItalic"},text:"Hai impiegato "+myTime+" ms per sparare, mentre il tuo avversario ne ha impiegati "+vsTime+".",textAlign:"center",color:"#aa938b",width:"85%"}),d=Titanium.UI.createButton({title:"Chiudi",font:{fontSize:19},color:"#856C64",top:10});return d.addEventListener("click",function(){e.close()}),n.playSound=function(){setTimeout(function(){o.play()},1500)},(999999999==myTime||888888888==myTime||999999999==vsTime||888888888==vsTime)&&(l.text=""),n.add(a),n.add(r),n.add(l),n.add(i),n.add(d),n}module.exports=Win;