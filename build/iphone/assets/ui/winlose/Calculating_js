function Calculating(){var e=require("ui/customUI/PageTitle"),t=new e;t.initialize("Duello",30);var i=Titanium.UI.createView({width:"100%",height:"100%",backgroundColor:"#F5F4F2",layout:"vertical"}),o=Titanium.UI.createImageView({top:30,image:"images/boots.png",backgroundColor:"transparent",width:johnsLib.calculateWidth(250),height:johnsLib.calculateHeight(300)}),n=Titanium.UI.createLabel({top:50,font:{fontSize:21,fontFamily:"Roboto-Light"},text:"Sto determinando il vincitore, attendi...",textAlign:"center",color:"#856C64",width:"85%"});return i.add(t),i.add(o),i.add(n),i}module.exports=Calculating;