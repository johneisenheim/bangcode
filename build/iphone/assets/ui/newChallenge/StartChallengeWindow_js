function StartChallengeWindow(){var e=require("ui/newChallenge/StartChallengeView"),t=new e,i=require("ui/newChallenge/Picker"),n=new i,o=Titanium.UI.createWindow({navTintColor:"white",tintColor:"#E2BB5A",barColor:"#E2BB5A",extendEdges:[Ti.UI.EXTEND_EDGE_TOP],backgroundColor:"#F5F4F2",translucent:!0,statusBarStyle:Titanium.UI.iPhone.StatusBar.LIGHT_CONTENT,layout:"composite",backButtonTitle:String.format(L("main:back"),"")});return o.add(t),Ti.App.addEventListener("show_picker",function(){n.isShown||(o.add(n),n.animate({bottom:0,duration:300}),n.isShown=!0)}),Ti.App.addEventListener("close_picker",function(e){n.isShown&&n.animate({bottom:-250,duration:300},function(){o.remove(n),n.isShown=!1,t.getDateFromPicker(e.date)})}),Ti.App.addEventListener("back",function(){o.close()}),o}module.exports=StartChallengeWindow;