function StartChallengeWindow(){var e=require("ui/newChallenge/StartChallengeView"),t=new e,i=require("ui/newChallenge/Picker"),o=new i,n=Titanium.UI.createWindow({navTintColor:"white",tintColor:"#E2BB5A",barColor:"#E2BB5A",extendEdges:[Ti.UI.EXTEND_EDGE_TOP],backgroundColor:"#F5F4F2",translucent:!0,statusBarStyle:Titanium.UI.iPhone.StatusBar.LIGHT_CONTENT,layout:"composite",backButtonTitle:String.format(L("main:back"),"")});return n.add(t),Ti.App.addEventListener("show_picker",function(){o.isShown||(n.add(o),o.animate({bottom:0,duration:300}),o.isShown=!0)}),Ti.App.addEventListener("close_picker",function(e){o.isShown&&o.animate({bottom:-250,duration:300},function(){n.remove(o),o.isShown=!1,t.getDateFromPicker(e.date)})}),Ti.App.addEventListener("back",function(){n.close()}),n}module.exports=StartChallengeWindow;