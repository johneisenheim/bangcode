function SecondPage(){var e=require("login/facebook/FacebookHandler"),t=new e,i=Titanium.UI.createScrollView({width:Ti.UI.SIZE,height:Ti.UI.SIZE,backgroundColor:"#F5F4F2",layout:"vertical"}),o=Titanium.UI.createLabel({text:"F",font:{fontSize:490,fontFamily:"Wild West Icons"},color:"#856C64",top:0}),a=Titanium.UI.createLabel({top:-60,font:{fontSize:18,fontFamily:"Roboto-Light"},text:String.format(L("wizard:loginLabel"),""),textAlign:"center",color:"#856C64",width:"95%"}),n=Titanium.UI.createButton({title:String.format(L("wizard:loginButtonLabel"),""),color:"#856C64",font:{fontSize:23},top:25});return n.addEventListener("click",function(){loader.showLoader(i),t.logMeIn()}),i.add(o),i.add(a),i.add(n),i}module.exports=SecondPage;