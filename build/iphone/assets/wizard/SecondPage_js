function SecondPage(){var t=require("login/facebook/FacebookHandler"),e=new t,i=Titanium.UI.createView({width:Ti.UI.SIZE,height:Ti.UI.SIZE,backgroundColor:"#F5F4F2",layout:"vertical"}),o=Titanium.UI.createLabel({text:"F",font:{fontSize:490,fontFamily:"Wild West Icons"},color:"#856C64",top:0}),n=Titanium.UI.createLabel({top:-60,font:{fontSize:18,fontFamily:"Roboto-Light"},text:String.format(L("wizard:loginLabel"),""),textAlign:"center",color:"#856C64",width:"95%"}),a=Titanium.UI.createButton({title:String.format(L("wizard:loginButtonLabel"),""),color:"#856C64",font:{fontSize:23},top:25});return a.addEventListener("click",function(){e.logMeIn()}),i.add(o),i.add(n),i.add(a),i}module.exports=SecondPage;