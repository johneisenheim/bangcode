function Wizard(){var e=require("wizard/FirstPage"),t=require("wizard/SecondPage"),i=new e,o=new t,a=Titanium.UI.createWindow({width:"100%",height:"100%",statusBarStyle:Titanium.UI.iPhone.StatusBar.GREY,navBarHidden:!0}),n=Ti.UI.createScrollableView({views:[i,o],showPagingControl:!1,scrollingEnabled:!0,width:"100%",height:"100%",layout:"composite",backgroundColor:"#F5F4F2"});return a.add(n),a}module.exports=Wizard;